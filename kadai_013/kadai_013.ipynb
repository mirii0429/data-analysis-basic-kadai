      "source": [
        "#整数の引数priceと引数tax_rateを指定した関数calculate_totalを定義する。\n",
        "#税込金額を戻り値として返すようにする。\n",
        "def calculate_total(price: int, tax_rate: int):\n",
        "  return price * (1 + tax_rate / 100)\n",
        "\n",
        "#引数に110円と10%を代入した合計を変数totalとする。\n",
        "#税込金額を表示する。\n",
        "total = calculate_total(110, 10)\n",
        "print(f\"税込金額は{total}円です。\")\n"
      ]
    }
